<?xml version="1.0"?>
<launch>
    <!-- Add all launch files that you want to run additionally to your robot bringup like moveit, etc... -->

    <!-- args -->
    <arg name="cob_simuliation_launch" default="$(find cob_bringup_sim)/launch"/>
    <arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>
    <arg name="robot_env" default="$(optenv ROBOT_ENV !!NO_ROBOT_ENV_SET!!)"/>
    <arg name="pkg_nav_config" default="$(find ipa_navigation_config)"/>
    <arg name="pkg_env_config" default="$(find cob_default_env_config)" />
    <arg name="pkg_nav_env_config" default="$(find ipa_navigation_environment_config)"/>

    <arg name="gazebo_gui" default="false"/>

    <!-- launch files -->
    <include file="$(find cob_bringup_sim)/launch/robot.launch">
      <arg name="gui" default="$(arg gazebo_gui)"/>
    </include>
    <include file="$(find ipa_navigation_bringup)/ipa_navigation.launch"/>

</launch>
